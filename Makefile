all: timelogger$(EXEEXT)

install: install_timelogger
	$(INSTALL_DIR) $(DESTDIR)$(datadir)
	for f in locale/pl/LC_MESSAGES/timelogger.mo locale/es/LC_MESSAGES/timelogger.mo locale/ru/LC_MESSAGES/timelogger.mo locale/pt/LC_MESSAGES/timelogger.mo; do \
	if test ! -d $(DESTDIR)$(datadir)/`dirname $$f` ; then \
	$(INSTALL_DIR) $(DESTDIR)$(datadir)/`dirname $$f`; \
	fi; \
	$(INSTALL_DATA) share/$$f $(DESTDIR)$(datadir)/$$f; \
	done
	$(INSTALL_DIR) $(DESTDIR)$(datadir)/applications
	(cd $(srcdir)/src ; $(INSTALL_DATA)  timelogger.desktop $(DESTDIR)$(datadir)/applications)
	$(INSTALL_DIR) $(DESTDIR)$(datadir)/pixmaps
	(cd $(srcdir)/src ; $(INSTALL_DATA)  timelogger.png $(DESTDIR)$(datadir)/pixmaps)

uninstall: uninstall_timelogger
	for f in locale/pl/LC_MESSAGES/timelogger.mo locale/es/LC_MESSAGES/timelogger.mo locale/ru/LC_MESSAGES/timelogger.mo locale/pt/LC_MESSAGES/timelogger.mo; do \
	rm -f $(DESTDIR)$(datadir)/$$f; \
	done
	(cd $(DESTDIR)$(datadir)/applications ; rm -f timelogger.desktop)
	(cd $(DESTDIR)$(datadir)/pixmaps ; rm -f timelogger.png)

install-strip: install
	$(STRIP) $(DESTDIR)$(bindir)/timelogger$(EXEEXT)

clean: 
	rm -rf ./.deps ./.pch
	rm -f ./*.o
	rm -f timelogger$(EXEEXT)

distclean: clean
	rm -f config.cache config.log config.status bk-deps bk-make-pch shared-ld-sh Makefile

